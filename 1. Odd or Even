num = int(input("Please enter a number:"))
check = int(input("Please enter a 2nd number:"))
is_even = num % 2
if is_even > 0 :
    print("Num is odd!")
else:
    print("Num is even!")
numandcheck = num/check
if numandcheck % 2 == 0:
    print("It divides into num!")
else:
    print("Did not divide evenly:(")
